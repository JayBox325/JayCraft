{
  "name": "jaybox325/jaycraft",
  "description": "Craft boilerplate to work with JayBox325/JayPack",
  "version": "1.1.0",
  "type": "project",
  "license": "Unlicense",
  "require": {
    "barrelstrength/sprout-seo": "4.6.9",
    "craftcms/cms": "3.7.3",
    "craftcms/redactor": "2.8.7",
    "ether/tags": "^1.0",
    "marionnewlevant/snitch": "3.0.4.1",
    "mmikkel/cp-field-inspect": "1.2.5",
    "nystudio107/craft-similar": "1.1.5",
    "nystudio107/craft-typogrify": "^1.1.1",
    "ostark/craft-plugin-commands": "^1.1",
    "percipioglobal/craft-colour-swatches": "1.4.1.1",
    "sebastianlenz/linkfield": "1.0.25",
    "spicyweb/craft-embedded-assets": "2.7.0",
    "vaersaagod/matrixmate": "^1.2",
    "verbb/cp-nav": "3.0.17",
    "verbb/navigation": "1.4.19",
    "verbb/super-table": "2.6.8",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.3.8"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
